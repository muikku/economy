{
  "name": "economy",
  "version": "1.0.0",
  "description": "How to use currently:",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently --kill-others \"nodemon server.js\" \"npm run client\"",
    "client": "npm start --prefix client",
    "build:ui": "rmdir client/build && cd client && npm run build",
    "deploy": "git push heroku master",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muikku/economy.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/muikku/economy/issues"
  },
  "homepage": "https://github.com/muikku/economy#readme",
  "devDependencies": {
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1"
  }
}
